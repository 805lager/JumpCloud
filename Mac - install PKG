AppName=""
PKGName=""
URLName=""

#Make temp folder for downloads. 
mkdir "/tmp/$AppName/"; cd "/tmp/$AppName/"; 

#Download Tableau. 
curl -L -o /tmp/$AppName/$PKGName.pkg "$URLName"; 

#install Tableau
sudo installer -pkg /private/tmp/$AppName/$PKGName.pkg -target /; 

#tidy up 
sudo rm -rf "/tmp/$AppName";
exit (0)
